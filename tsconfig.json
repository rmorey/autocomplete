{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "./build",
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "*": ["*", "node_modules/@withfig/autocomplete-tools/@types/*"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@withfig/autocomplete-tools/@types"
    ]
  },
  "include": ["./src/**/*", "dangerfile.ts"]
}
